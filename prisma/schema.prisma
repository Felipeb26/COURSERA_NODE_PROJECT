generator client{
    provider = "prisma-client-js"
}

datasource db {
    provider ="mongodb"
    url = env("DATABASE_URL")
}

model Employee{
    id String @id @default(auto()) @map("_id") @db.ObjectId
    name String
    surname String
    email String @unique
    department String
    birth_date DateTime? @default(now())
    create_at DateTime? @default(now())
    @@map("employee")
}